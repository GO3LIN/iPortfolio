<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>usersPL</managed-bean-name>
  <managed-bean-class>com.iporfilio.beans.UsersPL</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>userDAO</property-name>
   <property-class>com.iportfolio.users.UsersDAO</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>realisationsPL</managed-bean-name>
  <managed-bean-class>com.iporfilio.beans.RealisationsPL</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>realisationDAO</property-name>
   <property-class>com.iportfolio.realisations.RealisationsDAO</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>commentairesPL</managed-bean-name>
  <managed-bean-class>com.iporfilio.beans.CommentairesPL</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>commentaireDAO</property-name>
   <property-class>com.iportfolio.commentaires.CommentairesDAO</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>userDAO</property-name>
   <property-class>com.iportfolio.commentaires.CommentairesDAO</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>categoriesPL</managed-bean-name>
  <managed-bean-class>com.iporfilio.beans.CategoriesPL</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>categoriesDAO</property-name>
   <property-class>com.iportfolio.categories.CategoriesDAO</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/welcome.jsp</from-view-id>
  <navigation-case>
   <from-outcome>failure</from-outcome>
   <to-view-id>/welcome.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/loginUser.jsp</from-view-id>
  <navigation-case>
   <from-outcome>sucsses</from-outcome>
   <to-view-id>/welcome.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <factory>
  <application-factory>com.iporfilio.beans.CommentairesPL</application-factory>
 </factory>
</faces-config>
